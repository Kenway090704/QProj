<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aoben.qproj">


    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".ui.QProjApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon1024"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".ui.SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.MainActivity" />
        <activity android:name=".ui.SalesmanActivity" />
        <activity android:name=".ui.AllProductActivity" />
        <activity android:name=".ui.ProductDetailActivity" />

    </application>

</manifest>